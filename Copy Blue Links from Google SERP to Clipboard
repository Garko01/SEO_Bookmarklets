// Copies all "blue links" on the Google search result page to the clipboard as plain text (no alerts). These are anchor (<a>) elements with the attribute jsname="UWckNb"
// Note: This attribute is specific to Google's current layout and could change if Google updates their page structure. 
javascript:(function() { var links = document.querySelectorAll('a[jsname="UWckNb"]'); var excludeSelectors = ['.Wt5Tfe a[jsname="UWckNb"]']; var urls = []; links.forEach(function(link) { var exclude = false; excludeSelectors.forEach(function(selector) { if (link.closest(selector)) { exclude = true; } }); if (!exclude) { urls.push(link.href); } }); if (urls.length > 0) { var urlsText = urls.join('\n'); navigator.clipboard.writeText(urlsText); }})();
